CREATE TABLE `wp_coe_pipette_xml_imports` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `certificate_number` varchar(60) DEFAULT NULL,
  `serial_number` varchar(30) DEFAULT NULL,
  `pipette_id` varchar(30) DEFAULT NULL,
  `client_id` int(10) unsigned DEFAULT NULL,
  `client_name` varchar(60) DEFAULT NULL,
  `xml_schema_type` varchar(20) DEFAULT 'METTLER',
  `certificate_data` longtext,
  `result` varchar(8) DEFAULT 'PENDING',
  `comment` text,
  `validity` tinyint(4) DEFAULT '12',
  `date_performed` date DEFAULT NULL,
  `created_at` datetime DEFAULT CURRENT_TIMESTAMP,
  `created_by` int(10) unsigned DEFAULT NULL,
  `verified_at` datetime DEFAULT NULL,
  `verified_by` int(10) unsigned DEFAULT NULL,
  `approved_at` datetime DEFAULT NULL,
  `approved_by` int(10) unsigned DEFAULT NULL,
  PRIMARY KEY (`id`)
);
